{
  "name": "@clawdvault/cli",
  "version": "0.3.2",
  "description": "CLI for ClawdVault - Solana token launchpad",
  "main": "dist/index.js",
  "bin": {
    "clawdvault": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --noEmit && tsup src/index.ts --format cjs --clean --shims",
    "dev": "tsup src/index.ts --format cjs --watch --shims",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@clawdvault/sdk": "^0.3.0",
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.91.0",
    "bip39": "^3.1.0",
    "bs58": "^6.0.0",
    "chalk": "^4.1.2",
    "commander": "^14.0.3",
    "ora": "^9.3.0",
    "cli-table3": "^0.6.3"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "clawdvault",
    "solana",
    "cli",
    "token",
    "launchpad"
  ],
  "author": "ClawdVault",
  "license": "MIT"
}
